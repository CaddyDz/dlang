export PATH=$HOME/bin:/usr/local/bin:~/.composer/vendor/bin:/usr/local/Cellar/nethogs/0.8.5/sbin:$PATH
export PATH=$HOME/Downloads/Apps:$PATH
export ZSH=$HOME/.oh-my-zsh
